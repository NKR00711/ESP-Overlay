<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape android:dither="true" android:visible="true" android:shape="rectangle" android:useLevel="false">
            <gradient android:startColor="#ffffffff" android:endColor="#ffffffff" android:angle="270.0" />
            <corners android:radius="12.5dip" />
            <size android:height="10.0dip" android:width="10.0dip" />
            <stroke android:width="3.0dip" android:color="#ff202022" />
        </shape>
    </item>
    <item android:state_checked="false">
        <shape android:dither="true" android:visible="true" android:shape="rectangle" android:useLevel="false">
            <gradient android:startColor="@color/accent" android:endColor="@color/accent" android:angle="270.0" />
            <corners android:radius="12.5dip" />
            <size android:height="10.0dip" android:width="10.0dip" />
            <stroke android:width="3.0dip" android:color="#ff202022" />
        </shape>
    </item>
</selector>